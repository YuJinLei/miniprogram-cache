{
  "name": "@miniprogramjs/cache",
  "version": "1.1.0",
  "description": "一个适用于微信小程序的模块化缓存工具库",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "YuJinLei <1002580998@qq.com>",
  "license": "ISC",
  "repository": "https://github.com/YuJinLei/miniprogram-cache",
  "keywords": [
    "@miniprogramjs",
    "@miniprogramjs-cache",
    "@miniprogram",
    "@miniprogram-cache",
    "miniprogram",
    "miniprogram-cache",
    "mini",
    "mini-cache",
    "cache"
  ],
  "directories": {
    "lib": "lib"
  },
  "jest": {
    "rootDir": "__test__",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__test__/.*|(\\.|/)test)test\\.ts?$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "scripts": {
    "dev": "yarn && tsc -w",
    "build": "rm -rf dist && yarn && tsc",
    "test": "jest --verbose"
  },
  "files": [
    "dist",
    "package.json",
    "CHANGELOG.md",
    "README.md"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/wechat-miniprogram": "^3.3.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.5",
    "typescript": "^4.1.3"
  }
}
